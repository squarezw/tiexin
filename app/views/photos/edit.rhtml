<div id="form_error" class="errors">
	
</div>           
            
<% remote_form_for :photo, @photo, :url => photo_path(owner_type(@owner), @owner.id, @photo), :method=>:put, :html=>{:method=>:put} do |f| -%>
<table class="form_container">
	<tr>
		<td width="15%"><label><%= s_('NaviPhoto|Subject')%></label></td>
		<td><%= f.text_field :subject, :size=>50 %></td>
	</tr>
</table>
<% end -%>

<%= update_page_tag do |page|
	   page.call 'dialog.show_with_title', _('Upload Photo') 
	end -%>
